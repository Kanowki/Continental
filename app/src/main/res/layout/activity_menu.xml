<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_menu"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.example.tanguygranat.continental.MenuActivity">

    <Button
        android:text="@string/meeting"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/button_office"
        android:layout_marginTop="40dp"
        android:id="@+id/button_meeting" />

    <Button
        android:text="@string/POI"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="37dp"
        android:id="@+id/button_poi"
        android:layout_below="@+id/button_meeting" />

    <Button
        android:text="@string/Around_me"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="50dp"
        android:id="@+id/button_aroundme"
        android:layout_below="@+id/button_poi" />

    <Button
        android:text="@string/office"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="62dp"
        android:id="@+id/button_office"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:onClick="onMeetingClick"/>
</RelativeLayout>
